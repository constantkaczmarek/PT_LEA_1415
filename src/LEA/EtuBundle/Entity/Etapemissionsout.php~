<?php

namespace LEA\EtuBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Etapemissionsout
 *
 * @ORM\Table(name="etapemissionsout")
 * @ORM\Entity
 */
class Etapemissionsout
{
    /**
     * @var string
     *
     * @ORM\Column(name="alternanceRef", type="string", length=80, nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $alternanceref;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="dateRencontre", type="date", nullable=false)
     */
    private $daterencontre;

    /**
     * @var boolean
     *
     * @ORM\Column(name="signatureEtud", type="boolean", nullable=false)
     */
    private $signatureetud;

    /**
     * @var string
     *
     * @ORM\Column(name="remarquesEtud", type="text", nullable=false)
     */
    private $remarquesetud;

    /**
     * @var boolean
     *
     * @ORM\Column(name="signatureTuteur", type="boolean", nullable=false)
     */
    private $signaturetuteur;

    /**
     * @var string
     *
     * @ORM\Column(name="remarquesTuteur", type="text", nullable=false)
     */
    private $remarquestuteur;

    /**
     * @var boolean
     *
     * @ORM\Column(name="signatureReferent", type="boolean", nullable=false)
     */
    private $signaturereferent;

    /**
     * @var string
     *
     * @ORM\Column(name="remarquesReferent", type="text", nullable=false)
     */
    private $remarquesreferent;

    /**
     * @var string
     *
     * @ORM\Column(name="typeRencontre", type="text", nullable=false)
     */
    private $typerencontre;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="dateValidation", type="date", nullable=false)
     */
    private $datevalidation;

    /**
     * @var string
     *
     * @ORM\Column(name="missions", type="text", nullable=false)
     */
    private $missions;

    /**
     * @var string
     *
     * @ORM\Column(name="environnementTechnique", type="text", nullable=false)
     */
    private $environnementtechnique;

    /**
     * @var string
     *
     * @ORM\Column(name="enjeux", type="text", nullable=false)
     */
    private $enjeux;



    /**
     * Get alternanceref
     *
     * @return string 
     */
    public function getAlternanceref()
    {
        return $this->alternanceref;
    }

    /**
     * Set daterencontre
     *
     * @param \DateTime $daterencontre
     * @return Etapemissionsout
     */
    public function setDaterencontre($daterencontre)
    {
        $this->daterencontre = $daterencontre;

        return $this;
    }

    /**
     * Get daterencontre
     *
     * @return \DateTime 
     */
    public function getDaterencontre()
    {
        return $this->daterencontre;
    }

    /**
     * Set signatureetud
     *
     * @param boolean $signatureetud
     * @return Etapemissionsout
     */
    public function setSignatureetud($signatureetud)
    {
        $this->signatureetud = $signatureetud;

        return $this;
    }

    /**
     * Get signatureetud
     *
     * @return boolean 
     */
    public function getSignatureetud()
    {
        return $this->signatureetud;
    }

    /**
     * Set remarquesetud
     *
     * @param string $remarquesetud
     * @return Etapemissionsout
     */
    public function setRemarquesetud($remarquesetud)
    {
        $this->remarquesetud = $remarquesetud;

        return $this;
    }

    /**
     * Get remarquesetud
     *
     * @return string 
     */
    public function getRemarquesetud()
    {
        return $this->remarquesetud;
    }

    /**
     * Set signaturetuteur
     *
     * @param boolean $signaturetuteur
     * @return Etapemissionsout
     */
    public function setSignaturetuteur($signaturetuteur)
    {
        $this->signaturetuteur = $signaturetuteur;

        return $this;
    }

    /**
     * Get signaturetuteur
     *
     * @return boolean 
     */
    public function getSignaturetuteur()
    {
        return $this->signaturetuteur;
    }

    /**
     * Set remarquestuteur
     *
     * @param string $remarquestuteur
     * @return Etapemissionsout
     */
    public function setRemarquestuteur($remarquestuteur)
    {
        $this->remarquestuteur = $remarquestuteur;

        return $this;
    }

    /**
     * Get remarquestuteur
     *
     * @return string 
     */
    public function getRemarquestuteur()
    {
        return $this->remarquestuteur;
    }

    /**
     * Set signaturereferent
     *
     * @param boolean $signaturereferent
     * @return Etapemissionsout
     */
    public function setSignaturereferent($signaturereferent)
    {
        $this->signaturereferent = $signaturereferent;

        return $this;
    }

    /**
     * Get signaturereferent
     *
     * @return boolean 
     */
    public function getSignaturereferent()
    {
        return $this->signaturereferent;
    }

    /**
     * Set remarquesreferent
     *
     * @param string $remarquesreferent
     * @return Etapemissionsout
     */
    public function setRemarquesreferent($remarquesreferent)
    {
        $this->remarquesreferent = $remarquesreferent;

        return $this;
    }

    /**
     * Get remarquesreferent
     *
     * @return string 
     */
    public function getRemarquesreferent()
    {
        return $this->remarquesreferent;
    }

    /**
     * Set typerencontre
     *
     * @param string $typerencontre
     * @return Etapemissionsout
     */
    public function setTyperencontre($typerencontre)
    {
        $this->typerencontre = $typerencontre;

        return $this;
    }

    /**
     * Get typerencontre
     *
     * @return string 
     */
    public function getTyperencontre()
    {
        return $this->typerencontre;
    }

    /**
     * Set datevalidation
     *
     * @param \DateTime $datevalidation
     * @return Etapemissionsout
     */
    public function setDatevalidation($datevalidation)
    {
        $this->datevalidation = $datevalidation;

        return $this;
    }

    /**
     * Get datevalidation
     *
     * @return \DateTime 
     */
    public function getDatevalidation()
    {
        return $this->datevalidation;
    }

    /**
     * Set missions
     *
     * @param string $missions
     * @return Etapemissionsout
     */
    public function setMissions($missions)
    {
        $this->missions = $missions;

        return $this;
    }

    /**
     * Get missions
     *
     * @return string 
     */
    public function getMissions()
    {
        return $this->missions;
    }

    /**
     * Set environnementtechnique
     *
     * @param string $environnementtechnique
     * @return Etapemissionsout
     */
    public function setEnvironnementtechnique($environnementtechnique)
    {
        $this->environnementtechnique = $environnementtechnique;

        return $this;
    }

    /**
     * Get environnementtechnique
     *
     * @return string 
     */
    public function getEnvironnementtechnique()
    {
        return $this->environnementtechnique;
    }

    /**
     * Set enjeux
     *
     * @param string $enjeux
     * @return Etapemissionsout
     */
    public function setEnjeux($enjeux)
    {
        $this->enjeux = $enjeux;

        return $this;
    }

    /**
     * Get enjeux
     *
     * @return string 
     */
    public function getEnjeux()
    {
        return $this->enjeux;
    }
}
