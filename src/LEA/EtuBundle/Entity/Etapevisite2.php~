<?php

namespace LEA\EtuBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Etapevisite2
 *
 * @ORM\Table(name="etapevisite2")
 * @ORM\Entity
 */
class Etapevisite2
{
    /**
     * @var string
     *
     * @ORM\Column(name="alternanceRef", type="string", length=80, nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $alternanceref;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="dateRencontre", type="date", nullable=false)
     */
    private $daterencontre;

    /**
     * @var boolean
     *
     * @ORM\Column(name="signatureEtud", type="boolean", nullable=false)
     */
    private $signatureetud;

    /**
     * @var string
     *
     * @ORM\Column(name="remarquesEtud", type="text", nullable=false)
     */
    private $remarquesetud;

    /**
     * @var boolean
     *
     * @ORM\Column(name="signatureTuteur", type="boolean", nullable=false)
     */
    private $signaturetuteur;

    /**
     * @var string
     *
     * @ORM\Column(name="remarquesTuteur", type="text", nullable=false)
     */
    private $remarquestuteur;

    /**
     * @var boolean
     *
     * @ORM\Column(name="signatureReferent", type="boolean", nullable=false)
     */
    private $signaturereferent;

    /**
     * @var string
     *
     * @ORM\Column(name="remarquesReferent", type="text", nullable=false)
     */
    private $remarquesreferent;

    /**
     * @var string
     *
     * @ORM\Column(name="dateProbableSoutenance", type="text", nullable=false)
     */
    private $dateprobablesoutenance;

    /**
     * @var string
     *
     * @ORM\Column(name="avancementProjet", type="text", nullable=false)
     */
    private $avancementprojet;

    /**
     * @var string
     *
     * @ORM\Column(name="pointsPositifs", type="text", nullable=false)
     */
    private $pointspositifs;

    /**
     * @var string
     *
     * @ORM\Column(name="pointsProgres", type="text", nullable=false)
     */
    private $pointsprogres;



    /**
     * Get alternanceref
     *
     * @return string 
     */
    public function getAlternanceref()
    {
        return $this->alternanceref;
    }

    /**
     * Set daterencontre
     *
     * @param \DateTime $daterencontre
     * @return Etapevisite2
     */
    public function setDaterencontre($daterencontre)
    {
        $this->daterencontre = $daterencontre;

        return $this;
    }

    /**
     * Get daterencontre
     *
     * @return \DateTime 
     */
    public function getDaterencontre()
    {
        return $this->daterencontre;
    }

    /**
     * Set signatureetud
     *
     * @param boolean $signatureetud
     * @return Etapevisite2
     */
    public function setSignatureetud($signatureetud)
    {
        $this->signatureetud = $signatureetud;

        return $this;
    }

    /**
     * Get signatureetud
     *
     * @return boolean 
     */
    public function getSignatureetud()
    {
        return $this->signatureetud;
    }

    /**
     * Set remarquesetud
     *
     * @param string $remarquesetud
     * @return Etapevisite2
     */
    public function setRemarquesetud($remarquesetud)
    {
        $this->remarquesetud = $remarquesetud;

        return $this;
    }

    /**
     * Get remarquesetud
     *
     * @return string 
     */
    public function getRemarquesetud()
    {
        return $this->remarquesetud;
    }

    /**
     * Set signaturetuteur
     *
     * @param boolean $signaturetuteur
     * @return Etapevisite2
     */
    public function setSignaturetuteur($signaturetuteur)
    {
        $this->signaturetuteur = $signaturetuteur;

        return $this;
    }

    /**
     * Get signaturetuteur
     *
     * @return boolean 
     */
    public function getSignaturetuteur()
    {
        return $this->signaturetuteur;
    }

    /**
     * Set remarquestuteur
     *
     * @param string $remarquestuteur
     * @return Etapevisite2
     */
    public function setRemarquestuteur($remarquestuteur)
    {
        $this->remarquestuteur = $remarquestuteur;

        return $this;
    }

    /**
     * Get remarquestuteur
     *
     * @return string 
     */
    public function getRemarquestuteur()
    {
        return $this->remarquestuteur;
    }

    /**
     * Set signaturereferent
     *
     * @param boolean $signaturereferent
     * @return Etapevisite2
     */
    public function setSignaturereferent($signaturereferent)
    {
        $this->signaturereferent = $signaturereferent;

        return $this;
    }

    /**
     * Get signaturereferent
     *
     * @return boolean 
     */
    public function getSignaturereferent()
    {
        return $this->signaturereferent;
    }

    /**
     * Set remarquesreferent
     *
     * @param string $remarquesreferent
     * @return Etapevisite2
     */
    public function setRemarquesreferent($remarquesreferent)
    {
        $this->remarquesreferent = $remarquesreferent;

        return $this;
    }

    /**
     * Get remarquesreferent
     *
     * @return string 
     */
    public function getRemarquesreferent()
    {
        return $this->remarquesreferent;
    }

    /**
     * Set dateprobablesoutenance
     *
     * @param string $dateprobablesoutenance
     * @return Etapevisite2
     */
    public function setDateprobablesoutenance($dateprobablesoutenance)
    {
        $this->dateprobablesoutenance = $dateprobablesoutenance;

        return $this;
    }

    /**
     * Get dateprobablesoutenance
     *
     * @return string 
     */
    public function getDateprobablesoutenance()
    {
        return $this->dateprobablesoutenance;
    }

    /**
     * Set avancementprojet
     *
     * @param string $avancementprojet
     * @return Etapevisite2
     */
    public function setAvancementprojet($avancementprojet)
    {
        $this->avancementprojet = $avancementprojet;

        return $this;
    }

    /**
     * Get avancementprojet
     *
     * @return string 
     */
    public function getAvancementprojet()
    {
        return $this->avancementprojet;
    }

    /**
     * Set pointspositifs
     *
     * @param string $pointspositifs
     * @return Etapevisite2
     */
    public function setPointspositifs($pointspositifs)
    {
        $this->pointspositifs = $pointspositifs;

        return $this;
    }

    /**
     * Get pointspositifs
     *
     * @return string 
     */
    public function getPointspositifs()
    {
        return $this->pointspositifs;
    }

    /**
     * Set pointsprogres
     *
     * @param string $pointsprogres
     * @return Etapevisite2
     */
    public function setPointsprogres($pointsprogres)
    {
        $this->pointsprogres = $pointsprogres;

        return $this;
    }

    /**
     * Get pointsprogres
     *
     * @return string 
     */
    public function getPointsprogres()
    {
        return $this->pointsprogres;
    }
}
