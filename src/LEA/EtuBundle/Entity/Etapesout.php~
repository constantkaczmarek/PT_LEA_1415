<?php

namespace LEA\EtuBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Etapesout
 *
 * @ORM\Table(name="etapesout")
 * @ORM\Entity
 */
class Etapesout
{
    /**
     * @var string
     *
     * @ORM\Column(name="alternanceRef", type="string", length=80, nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $alternanceref;

    /**
     * @var float
     *
     * @ORM\Column(name="pourcentageObjectifs", type="float", precision=10, scale=0, nullable=false)
     */
    private $pourcentageobjectifs;

    /**
     * @var string
     *
     * @ORM\Column(name="difficulteMission", type="text", nullable=false)
     */
    private $difficultemission;

    /**
     * @var float
     *
     * @ORM\Column(name="noteTravail", type="float", precision=10, scale=0, nullable=false)
     */
    private $notetravail;

    /**
     * @var float
     *
     * @ORM\Column(name="noteRapport", type="float", precision=10, scale=0, nullable=false)
     */
    private $noterapport;

    /**
     * @var float
     *
     * @ORM\Column(name="noteSoutenance", type="float", precision=10, scale=0, nullable=false)
     */
    private $notesoutenance;

    /**
     * @var string
     *
     * @ORM\Column(name="avisJury", type="text", nullable=false)
     */
    private $avisjury;



    /**
     * Get alternanceref
     *
     * @return string 
     */
    public function getAlternanceref()
    {
        return $this->alternanceref;
    }

    /**
     * Set pourcentageobjectifs
     *
     * @param float $pourcentageobjectifs
     * @return Etapesout
     */
    public function setPourcentageobjectifs($pourcentageobjectifs)
    {
        $this->pourcentageobjectifs = $pourcentageobjectifs;

        return $this;
    }

    /**
     * Get pourcentageobjectifs
     *
     * @return float 
     */
    public function getPourcentageobjectifs()
    {
        return $this->pourcentageobjectifs;
    }

    /**
     * Set difficultemission
     *
     * @param string $difficultemission
     * @return Etapesout
     */
    public function setDifficultemission($difficultemission)
    {
        $this->difficultemission = $difficultemission;

        return $this;
    }

    /**
     * Get difficultemission
     *
     * @return string 
     */
    public function getDifficultemission()
    {
        return $this->difficultemission;
    }

    /**
     * Set notetravail
     *
     * @param float $notetravail
     * @return Etapesout
     */
    public function setNotetravail($notetravail)
    {
        $this->notetravail = $notetravail;

        return $this;
    }

    /**
     * Get notetravail
     *
     * @return float 
     */
    public function getNotetravail()
    {
        return $this->notetravail;
    }

    /**
     * Set noterapport
     *
     * @param float $noterapport
     * @return Etapesout
     */
    public function setNoterapport($noterapport)
    {
        $this->noterapport = $noterapport;

        return $this;
    }

    /**
     * Get noterapport
     *
     * @return float 
     */
    public function getNoterapport()
    {
        return $this->noterapport;
    }

    /**
     * Set notesoutenance
     *
     * @param float $notesoutenance
     * @return Etapesout
     */
    public function setNotesoutenance($notesoutenance)
    {
        $this->notesoutenance = $notesoutenance;

        return $this;
    }

    /**
     * Get notesoutenance
     *
     * @return float 
     */
    public function getNotesoutenance()
    {
        return $this->notesoutenance;
    }

    /**
     * Set avisjury
     *
     * @param string $avisjury
     * @return Etapesout
     */
    public function setAvisjury($avisjury)
    {
        $this->avisjury = $avisjury;

        return $this;
    }

    /**
     * Get avisjury
     *
     * @return string 
     */
    public function getAvisjury()
    {
        return $this->avisjury;
    }
}
